{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Um clone do https://tabnews.com.br feito atr√°ves do https://curso.dev do Filipe Deschamps.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run docker:up && next dev",
    "docker:up": "docker compose --file 'infra/compose.yaml' up -d",
    "docker:stop": "docker compose --file 'infra/compose.yaml' stop",
    "docker:down": "docker compose --file 'infra/compose.yaml' down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.0.1"
  }
}
